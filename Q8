#include <stdio.h>

int main() {
    int val[6];
    
   
    for (int i = 0; i < 6; i++) {
        printf("Digite os valores: " );
        scanf("%d", &val[i]);
    }
    
    
    printf("Valores na ordem inversa: ");
    for (int i = 5; i >= 0; i--) {
        printf("%d ", val[i]);
    }
    
    return 0;
}
